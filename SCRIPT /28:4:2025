CREATE DATABASE FELIPE_BD
USE FELIPE_BD;

CREATE TABLE Estudiantes(
id int primary key,
nombre  varchar(50),
fechaNacimiento date
);
CREATE TABLE MATERIA (
CODIGO INT PRIMARY KEY IDENTITY(1,1),
NOMBRE VARCHAR(50),
);

CREATE TABLE INSCRIPCION(
ESTUDIANTEID INT,
MATERIACODIGO INT,
FOREIGN KEY (ESTUDIANTEID) REFERENCES Estudiantes(ID),
FOREIGN KEY (MATERIACODIGO) REFERENCES MATERIA(CODIGO));
SELECT * FROM INSCRIPCION;
INSERT INTO INSCRIPCION (ESTUDIANTEID,MATERIACODIGO)
VALUES(1,2);

INSERT INTO MATERIA (NOMBRE) VALUES ('MANTENIMIENTO III'),('PROGAMACION');
SELECT* FROM MATERIA;

DELETE FROM MATERIA
WHERE CODIGO=21;
DELETE FROM MATERIA
WHERE CODIGO=22;


SELECT * FROM ESTUDIANTES;
INSERT INTO Estudiantes (ID, Nombre, FechaNacimiento)
VALUES (1, 'Valentino', '2006-11-10'),
(2,'Santiago','2006-10-19');

DELETE FROM ESTUDIANTES
WHERE ID=1;
